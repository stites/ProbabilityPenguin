---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Sam Stites</title>
 <link href="http://stites.io/atom.xml" rel="self"/>
 <link href="http://stites.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://stites.io/</id>
 <author>
   <name>Sam Stites</name>
   <email>sam@stites.io</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://stites.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://stites.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>